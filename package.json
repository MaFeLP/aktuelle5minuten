    "bootstrap-icons": "^1.11.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "sass": "^1.78.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.9"
